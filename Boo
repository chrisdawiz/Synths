</Cabbage>
<CsoundSynthesizer>
<CsOptions>
-n -d -+rtmidi=NULL -M0 --midi-key-cps=4 --midi-velocity-amp=5 -m0d
</CsOptions>
<CsInstruments>
sr = 44100
ksmps = 64
nchnls = 2
0dbfs=1

instr Main ;Name
kMod chnget	"mod" ;Modulation control
kTune chnget "Detune" ;Detune conrtol
kPitch chnget "checkbox" ;Pitch control
kVol chnget "volume" ;Volume Control
kcf chnget "Cutoff" 
iAtt chnget "Attack"
iDec chnget "Decay"
iSus chnget "Sustain"
iRel chnget "Release"


kFilter madsr iAtt, iDec, iSus, iRel ;Filter controls
iAtt2 chnget "attack"
iDec2 chnget "decay"
iSus2 chnget "sustain"
iRel2 chnget "release"
kAmp madsr iAtt2, iDec2, iSus2, iRel2 ;Amp Controls

a1 vco2 p5*kVol, p4*kTune ;VCO1 controled by detune and volume
a2 vco2 p5*kVol, p4*kTune+kPitch ;VCO2 with similiar function but also featuring the pitch control
aMoog moogladder a1+a2, kFilter*kcf*kMod, 0.5 ;VCO1&2 summed through the moog filter with filter control and Modulation control

out aMoog*kAmp, aMoog*kAmp ;Outputs of the synth
endin

</CsInstruments>  
<CsScore>
f1 0 1024 10 1
f0 3600
</CsScore>
</CsoundSynthesizer>
